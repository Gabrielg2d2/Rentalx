{
  "name": "rentalx",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "test": "npm run mocha",
    "commit": "git-cz",
    "lint": "./node_modules/.bin/eslint . --max-warnings=0",
    "coverage": "nyc npm run test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "git-commit-msg-linter": "^4.2.1",
    "git-cz": "^4.7.6",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint --fix",
      "eslint --cache --fix",
      "prettier --write"
    ],
    "src/*.ts": [
      "npm run lint --fix",
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npx nyc check-coverage"
    }
  }
}
